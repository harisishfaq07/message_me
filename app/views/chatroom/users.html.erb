<br><br>
<div class= "ui container">
<table class="ui celled striped table">
  <thead>
    <tr><th colspan="3">
      Users
    </th>
  </tr></thead>
  <tbody>
  <% @users.each do |u| %>
    <tr>
      <td class="collapsing">
        <i class="user icon"></i>
      </td>
      <td><%= u.email %></td>
      <td class="right aligned collapsing"><%= u.created_at.strftime("%Y-%m-%d")%></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>